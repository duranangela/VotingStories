<section>
  <p>All Articles</p>
  <div class="articles">
    <div class='article'>
      <% @presenter.articles.each do |article| %>
        <% if current_user %>
          <p><%= check_box_tag 'title', article %><%= article[0] %></p>
        <% else %>
          <p><%= article[0] %></p>
        <% end %>
        <p><%= link_to article[1], article[2] %></p>
        <br>
      <% end %>
    </div>
  </div>

  <div class='favorites'>
  
  </div>
</section>


<%= javascript_include_tag "application.js" %>
