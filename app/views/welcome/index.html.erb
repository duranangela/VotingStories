<section>
  <div class="articles">
    <p>All Articles</p>
    <div class='article'>
      <% @presenter.articles.each do |article| %>
        <% if current_user %>
          <p><input type="checkbox"><%= article[:title] %></p>
        <% else %>
          <p><%= article[:title] %></p>
        <% end %>
        <p><%= link_to article[:source][:name], article[:url] %></p>
        <br>
      <% end %>
    </div>
  </div>
</section>
